{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfpeterdee"
		},
		"ADLS_peterdee_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_peterdee'"
		},
		"ADSL_pd_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADSL_pd'"
		},
		"BlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobStorage'"
		},
		"Blobstoragge_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Blobstoragge'"
		},
		"SqlDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlDB'"
		},
		"Staging_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Staging'"
		},
		"leyland_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'leyland'"
		},
		"ADLS_peterdee_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://peterdee007.dfs.core.windows.net"
		},
		"ADSL_pd_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlspeterdee.dfs.core.windows.net"
		},
		"Lego_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cdn.rebrickable.com/media/downloads/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfpeterdee_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_peterdee_accountKey": {
						"value": "[parameters('ADLS_peterdee_accountKey')]"
					},
					"ADSL_pd_accountKey": {
						"value": "[parameters('ADSL_pd_accountKey')]"
					},
					"Blobstoragge_connectionString": {
						"value": "[parameters('Blobstoragge_connectionString')]"
					},
					"SqlDB_connectionString": {
						"value": "[parameters('SqlDB_connectionString')]"
					},
					"Staging_connectionString": {
						"value": "[parameters('Staging_connectionString')]"
					},
					"BlobStorage_connectionString": {
						"value": "[parameters('BlobStorage_connectionString')]"
					},
					"leyland_connectionString": {
						"value": "[parameters('leyland_connectionString')]"
					},
					"ADLS_peterdee_properties_typeProperties_url": {
						"value": "[parameters('ADLS_peterdee_properties_typeProperties_url')]"
					},
					"ADSL_pd_properties_typeProperties_url": {
						"value": "[parameters('ADSL_pd_properties_typeProperties_url')]"
					},
					"Lego_properties_typeProperties_url": {
						"value": "[parameters('Lego_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adfpeterdee_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adfpeterdee_ArmTemplate_0')]"
			]
		}
	]
}